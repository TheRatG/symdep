{
    "name": "therat/symdep",
    "description": "Additional tool for symfony deploy",
    "require": {
        "php": ">=5.4.0",
        "symfony/finder": "~2.3",
        "symfony/console": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/process": "~2.3",
        "deployer/deployer": "~3.0",
        "phpseclib/phpseclib": "dev-master#62fdd160e3ae2bb265a7e1f7c9426d552f55df47",
        "herrera-io/phar-update": "~2.0"
    },
    "suggest": {
        "ext-ssh2": "Need for deployer"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Pak",
            "email": "the_rat_spam@mail.ru"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {"TheRat\\SymDep\\": "src/"}
    },
    "bin": [
        "bin/symdep",
        "bin/update",
        "bin/deploy_on_test",
        "bin/deploy_on_prod"
    ],
    "config": {
        "bin-dir": "bin"
    }
}
