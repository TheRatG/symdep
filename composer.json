{
    "name": "therat/symdep",
    "description": "Additional tool for symfony deploy",
    "require": {
        "ext-ssh2": "*",
        "symfony/finder": "2.6.*",
        "symfony/console": "2.6.*",
        "symfony/yaml": "2.6.*",
        "symfony/process": "2.6.*",
        "symfony/dependency-injection": "2.6.*",
        "symfony/filesystem": "2.6.*",
        "symfony/framework-bundle": "2.6.*",
        "symfony/process": "2.6.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Vladimir Pak",
            "email": "the_rat_spam@mail.ru"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {"TheRat\\SymDep\\": "src/"}
    },
    "bin": [
        "symdep",
        "deployer.phar"
    ],
    "config": {
        "bin-dir": "bin"
    }
}
