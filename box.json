{
    "directories": [
        "recipe/",
        "src/"
    ],
    "blacklist": [
        "Tests"
    ],
    "files": [
        "LICENSE",
        "README.md",
        "vendor/autoload.php",
        "vendor/herrera-io/phar-update/res/schema.json"
    ],
    "finder": [
        {
            "name": [
                "*.php",
                "*.json",
                "*.exe"
            ],
            "exclude": [
                ".gitignore",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests"
            ],
            "notPath": [
                "phpunit/",
                "phpdocumentor/",
                "leaphub/phpcs-symfony2-standard",
                "phpro/grumphp",
                "squizlabs/php_codesniffer"
            ],
            "in": "vendor"
        }
    ],
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "git-version": "package_version",
    "main": "bin/symdep",
    "output": "symdep.phar",
    "stub": true,
    "chmod": "0755"
}
