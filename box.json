{
    "directories": ["src/"],
    "finder": [
        {
            "name": [
                "*.php",
                "*.json",
                "*.exe"
            ],
            "exclude": [
                ".gitignore",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests",
                "yaml"
            ],
            "in": ""
        }
    ],
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "git-version": "package_version",
    "main": "bin/symdep",
    "output": "symdep.phar",
    "stub": true,
    "chmod": "0755"
}
