{
    "directories": [
        "recipe/",
        "src/"
    ],
    "extract": true,
    "files": [
        "LICENSE",
        "README.md",
        "vendor/autoload.php",
        "vendor/herrera-io/phar-update/res/schema.json"
    ],
    "finder": [
        {
            "name": [
                "*.php",
                "*.json",
                "*.exe"
            ],
            "exclude": [
                ".gitignore",
                ".md",
                "phpunit",
                "Tester",
                "Tests",
                "tests"
            ],
            "notPath": [
                "phpunit/",
                "phpdocumentor/",
                "leaphub/phpcs-symfony2-standard",
                "phpro/grumphp",
                "squizlabs/php_codesniffer"
            ],
            "in": "vendor"
        }
    ],
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "replacements": {
        "manifest_url": "https://raw.githubusercontent.com/TheRatG/symdep/gh-pages/manifest.json",
        "phar_url": "https://github.com/TheRatG/symdep/releases/download/3.4.0/symdep.phar"
    },
    "compression": "GZ",
    "git-commit": "git-commit",
    "git-version": "package_version",
    "main": "bin/console",
    "output": "tmp/symdep.phar",
    "stub": true,
    "chmod": "0755"
}
